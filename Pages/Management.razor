@page "/management"

@implements IDisposable
@inject ProtectionService ProtectionService
@inject NavigationManager NavigationManager

<h1>Hello, world!</h1>

<video id="videoFeed" width="1920" height="1080" style="margin-left:-5000px" />

<div class="row">
	<div class="col">
		<ul id='personlist'>

			@foreach (var person in Persons)
			{
				<li>
					@person
				</li>
			}

		</ul>
	</div>

</div>


